<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>REQUIRED_FIELDS_NOT_NULL</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISNULL(Name)),
NOT(ISNULL(Description__c)),
NOT(ISNULL(Field_API_Name__c)),
NOT(ISNULL(Object_API_Name__c))
)</errorConditionFormula>
    <errorMessage>Required fields can't be empty.</errorMessage>
</ValidationRule>
